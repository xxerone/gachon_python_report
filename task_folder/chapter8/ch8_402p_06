class Person:
    def __init__(self, name, mobile = 0, office = 1, email = 0): #디폴트 아닌거는 전부 앞으로
        self.name = name
        self.mobile = mobile
        self.office = office
        self.email = email
        print(self.name, self.mobile, self.office, self.email)
        
    def __str__(self): #self 안쓰는거 조심
        #print(self.name, self.mobile, self.office, self.email)
        return self.name, self.mobile, self.office, self.email
    """

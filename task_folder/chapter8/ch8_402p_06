    def getEmail(self): #접근자 2
        #print(self.y)
        print("email get" + self.email)
        return self.email

    def setEmail(self, email): #설정자 1 / 변경할 때 필요
        #print(x)
        self.email = email
        print("email set" + self.email)

#p1 = Person("Kim", office = "1234567", email = "kim@company.com")
